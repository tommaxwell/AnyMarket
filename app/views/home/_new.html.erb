<%= form_for new_user_session_path, :options => {:id => "new_listing"}, :url => {:controller => "sessions",:action => "new"} do |f| %>
			<%= f.text_field :email, :id => "name_input", :autofocus => true, :required => true, :placeholder => "Email *.edu only", :class => "form_inputs" %><br />
			<%= f.text_field :password, :id => "name_input", :required => true, :placeholder => "Password", :class => "form_inputs" %><br />
			<%= f.submit "Login", :id => "submit",:id => "submit_item_btn" %>
			<% end %>