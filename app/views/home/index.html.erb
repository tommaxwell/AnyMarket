<% if signed_in?(:user) %>
  <h1>Your listings</h1>
  <% @user.products.each do |f| %>
    <p><%= link_to f.name, view_item_path(f.id) %></p>
  <% end %>
<% else %>
<h1>FUCK YOU</h1>
<% end %>
<%= yield %>
<%= javascript_include_tag "application" %>
<script type="text/javascript">
  $.get("/categories", null, "script");
</script>